@article{WhiJanDey2003,
language = {English},
copyright = {Compilation and indexing terms, Copyright 2013 Elsevier Inc.},
copyright = {Compendex},
title = {Hierarchical basis for stabilized finite element methods for compressible flows},
journal = {Computer Methods in Applied Mechanics and Engineering},
author = {Christian H. Whiting and Kenneth E. Jansen and Saikat Dey},
volume = {192},
number = {47-48},
year = {2003},
pages = {5167 - 5185},
issn = {00457825},
abstract = {The quest for more accurate numerical simulations of the Navier-Stokes equations has motivated the study of high-order piecewise-polynomial basis functions, also known as k-version finite element methods, as a means to attain this accuracy in the most cost effective manner. Stabilized finite element methods (e.g. streamline upwind Petrov Galerkin) have been proven to attain optimal rates of convergence for any polynomial order basis as well as perform well on a variety of flows including turbulence applications. We present a stabilized finite element formulation for fluid dynamics using mesh-entity based hierarchical basis functions. The implementation is shown to attain theoretical convergence results for the linear model problem as well as the Navier-Stokes equations. &copy; 2003 Elsevier B.V. All rights reserved.},
key = {Compressible flow},
keywords = {Finite element method;Fluid dynamics;Navier Stokes equations;Polynomials;Turbulence;},
note = "",
URL = "http://dx.doi.org/10.1016/j.cma.2003.07.011",
} 

